{
  "Qdrant": {
    "Endpoint": "http://index.nuviot.com:6333",
    "ApiKey": "",
    "Collection": "code_chunks_v2",
    "VectorSize": 3072,
    "Distance": "Cosine"
  },
  "Embeddings": {
    "Provider": "OpenAI", 
    "ApiKey": "", 
    "Model": "text-embedding-3-large",
    "BaseUrl": "https://api.openai.com" 
  },
  "Ingestion": {
    "MaxTokensPerChunk": 7000,
    "OverlapLines": 8,
    "RootPaths": [ "D:/nuviot/co.core", "D:/nuviot/cs.cloudstorage" ],
    "Include": [ "**/*.cs", "**/*.ts", "**/*.js", "**/*.md" ],
    "Exclude": [ "**/bin/**", "**/obj/**", "**/node_modules/**" ]
  }
}